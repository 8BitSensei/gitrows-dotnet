{
  "env": {
    "umd": {
      "plugins": ["@babel/plugin-transform-modules-umd"],
    },
		"node":{
			"plugins": [
				"add-module-exports",
			],

		}
  },
	"presets": [
			[
		"@babel/preset-env", {
			"useBuiltIns": "entry",
			"targets": {
            "browsers": [
              "last 2 Chrome versions",
              "last 2 Firefox versions",
              "last 2 Edge versions",
              "last 2 Opera versions",
              "last 2 Safari versions",
              "last 2 iOS versions"
            ],
					}
		}],
	],
}
